{
  "name": "reciple-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "rimraf ./modules && tsc",
    "start": "reciple",
    "test": "reciple -c ./reciple.test.yml",
    "build:start": "npm run build && reciple",
    "build:test": "npm run build && reciple -c ./reciple.test.yml",
    "dev": "nodemon --watch src --ext ts,mts,cts --exec \"npm run build && reciple -c ./reciple.test.yml\" --signal SIGHUP"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "discord.js": "^14.12.1",
    "fallout-utility": "^2.7.1",
    "github-slugger": "^2.0.0",
    "ms": "^2.1.3",
    "reciple": "^7.9.9",
    "reciple-anticrash": "^1.1.3",
    "reciple-dev-commands": "^1.3.9",
    "reciple-interaction-events": "^1.1.3",
    "reciple-registry-cache": "^1.0.9",
    "semver": "^7.5.4",
    "typedoc-json-parser": "^8.2.0",
    "uwuifier": "^4.0.5"
  },
  "devDependencies": {
    "@types/ms": "^0.7.31",
    "@types/node": "^20.2.5",
    "@types/semver": "^7.5.0",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.1",
    "typescript": "^5.0.4"
  }
}
